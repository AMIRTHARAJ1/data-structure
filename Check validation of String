 #include <stdio.h>
#include <stdlib.h>
int ValidateString (char *B)
{
        int i;
        for (i = 0; B[i] != '\0'; i++)
        {
               if (!(B[i] >= 65 && B[i] <= 90) && !(B[i] >= 97 && B[i] <= 122) && !(B[i] >= 48 && B[i] <= 57))
              {
                    return 0;
              }
       }
       return 1;
}
int main ()
{
	printf("Amirtharaj.k\n192321112\n");
        char *B = "amir@7928";
        int j;
        printf ("String is \"%s\"\n", B);
        if (ValidateString (B))
        {
              printf ("Valid String");
        }
        else
        {
             printf ("Invalid String");
        }
        return 0;
}
