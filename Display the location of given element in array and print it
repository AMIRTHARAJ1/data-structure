 #include <stdio.h>

int main() {
	printf("Amirtharaj\n192321112\n");
    int arr[] = {10, 20, 30, 40, 50};
    int n = sizeof(arr) / sizeof(arr[0]);
    int element = 30;
    int location = -1;

    for (int i = 0; i < n; i++) {
        if (arr[i] == element) {
            location = i;
            break;
        }
    }

    if (location != -1) {
        printf("The element %d is found at index %d in the array.\n", element, location);
    } else {
        printf("The element %d is not found in the array.\n", element);
    }

    return 0;
}
