 #include <stdio.h>

int main() {
	printf("Amirtharaj.k\n192321112\n");
    int n, i, sum = 0;
    int fib[100];

    printf("Enter the number of terms: ");
    scanf("%d", &n);

    fib[0] = 0;
    fib[1] = 1;

    for (i = 2; i < n; i++) {
        fib[i] = fib[i - 1] + fib[i - 2];
    }

    printf("Fibonacci Series: ");
    for (i = 0; i < n; i++) {
        printf("%d ", fib[i]);
        sum += fib[i];
    }

    printf("\nSum of Fibonacci Series: %d\n", sum);

    return 0;
}
