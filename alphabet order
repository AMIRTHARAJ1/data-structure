 #include <stdio.h>
#include <string.h>

void sortAlphabet(char* str) {
    int n = strlen(str);
    for (int i = 0; i < n-1; i++) {
        for (int j = 0; j < n-i-1; j++) {
            if (str[j] > str[j+1]) {
                char temp = str[j];
                str[j] = str[j+1];
                str[j+1] = temp;
            }
        }
    }
}

int main() {
	printf("Amirtharaj.k\n192321112\n");
    char str[] = "alphabet";
    sortAlphabet(str);
    printf("Alphabet in order: %s\n", str);
    return 0;
}
