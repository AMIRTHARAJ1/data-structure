 #include <stdio.h>

int main() {
	printf("Amirtharaj.k\n192321112\n");
    int matrix[3][3] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
    int sum = 0;

     for (int i = 0; i < 3; i++) {
        sum += matrix[i][i];
    }

     for (int i = 0; i < 3; i++) {
        sum += matrix[i][2 - i];
    }

    printf("The sum of the diagonals is: %d\n", sum);

    return 0;
}
